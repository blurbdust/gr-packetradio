<?xml version="1.0"?>
<block>
  <name>AX.25 Protocol Encoder</name>
  <key>packetradio_ax25_protocol_encode</key>
  <category>[packetradio]</category>
  <import>import packetradio</import>
  <make>packetradio.ax25_protocol_encode($dcall, $scall)</make>
  <param>
    <name>Source Callsign</name>
    <key>scall</key>
    <type>string</type>
  </param>
  <param>
    <name>Destination Callsign</name>
    <key>dcall</key>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
